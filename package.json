{
  "name": "LoanAppExample",
  "version": "0.0.1",
  "description": "4finance loan app example",
  "scripts": {
    "bundle-dev": "webpack --bail",
    "bundle-dev-watch": "webpack --watch",
    "bundle-prod": "set NODE_ENV=production && webpack --bail"
  },
  "babel": {
    "presets": [
      "react",
      "es2015",
      "stage-0"
    ],
    "env": {
      "production": {
        "plugins": [
          "transform-react-constant-elements",
          "transform-react-inline-elements"
        ]
      }
    }
  },
  "dependencies": {
    "classnames": "^2.2.3",
    "jquery": "2.2.3",
    "material-ui": "0.15.0-beta.2",
    "react": "15.0.1",
    "react-dom": "15.0.1",
    "react-flexgrid": "0.7.0",
    "react-tap-event-plugin": "1.0.0"
  },
  "devDependencies": {
    "babel": "6.5.2",
    "babel-core": "6.5.2",
    "babel-loader": "6.2.4",
    "babel-plugin-module-alias": "1.3.0",
    "babel-plugin-react-transform": "2.0.2",
    "babel-preset-es2015": "6.6.0",
    "babel-preset-react": "6.5.0",
    "babel-preset-react-hmre": "1.1.1",
    "babel-preset-stage-0": "6.5.0",
    "css-loader": "^0.23.1",
    "style-loader": "^0.13.1",
    "webpack": "1.13.0"
  }
}
